  <body>
    <!-- الايقنات -->
    <%- include('../components/dark-light.ejs') %>


    <main class="d-flex flex-nowrap">
      <!-- التحديد الي على اسم الصفحة الي بلون الأزرق -->
      <%- include('../components/sidebar.ejs', {currentPage:"add"}) %><!-- من ادواتة انك تقدر ترسل معه كي وفاليو ejs  ال  -->

      <section class="w-100">
        <%- include('../components/navbar.ejs') %>
       
        <form  action="/user/add.html" method="post" class="mx-0 row gx-3 gy-4 mt-3">
          <div class="col-md-6">
            <label for="inputfirstName4" class="form-label">First Name: </label>
            <input
            name="firstNamecustomer"
              placeholder="First Name..."
              type="text"
              class="form-control"
              id="inputfirstName4"
            />
          </div>
          
          <div class="col-md-6">
            <label for="inputlastName4" class="form-label">Last Name:</label>
            <input
            name="lastNamecustomer"
              placeholder="Last Name..."
              type="text"
              class="form-control"
              id="inputlastName4"
            />
          </div> 

          <div class="col-md-6">
            <label for="inputtele4" class="form-label">Telephone:</label>
            <input
            name="phoneNumber"
              placeholder="05XXXXXXXX"
              type="number"
              class="form-control"
              id="inputtele4"
            />
          </div>

          <input
          name="salesEmployeeId"
           
            type="hidden"
            value="<%= jwt.id %>"
      
          />

          <input
          name="salesEmployeeName"
           
            type="hidden"
      value="<%= jwt.name %>"
         
          />


          
          <input
          name="salesEmployeeUserName"
           
            type="hidden"
            value="<%= jwt.userName %>"
          
          />


        
            <!-- لستة الدول  -->
          <%- include('../components/country_list.ejs') %>

          <div class="col-12">
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </form>
      </section>
    </main>
    <script src="../js/bootstrap.bundle.min.js"></script>

    <script src="../js/sidebars.js"></script>
    <script src="../js/main.js"></script>
  </body>

